{
  "name": "site",
  "version": "0.1.0",
  "description": "metamob front-end",
  "keywords": [
    "Internet Computer",
    "Motoko",
    "JavaScript",
    "Canister"
  ],
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "prebuild": "npm run prod:generate",
    "dev:front": "npm run serve",
    "dev:back": "./scripts/deploy-backend.sh",
    "prod:front": "./scripts/deploy-frontend-prod.sh",
    "prod:back": "./scripts/deploy-backend-prod.sh",
    "serve": "webpack serve --host 0.0.0.0 --mode development --env development",
    "prestart": "npm run generate",
    "dev:generate": "dfx generate --identity default metamob & dfx generate --identity default logger & dfx generate --identity default emailer & dfx generate --identity default btcwallet & dfx generate --identity default mmt & dfx generate --identity default ledger & dfx generate --identity default --network ic site",
    "prod:generate": "dfx generate --identity default --network ic metamob & dfx generate --identity default --network ic logger & dfx generate --identity default --network ic emailer & dfx generate --identity default --network ic btcwallet & dfx generate --identity default --network ic mmt & dfx generate --identity default --network ic ledger & dfx generate --identity default --network ic site",
    "i18n:extract": "formatjs extract -- \"src/site/src/**/*.ts*\" --ignore=\"**/*.d.ts\" --out-file \"src/site/assets/lang/en.json\" --id-interpolation-pattern \"[sha512:contenthash:base64:6]\""
  },
  "devDependencies": {
    "@creativebulma/bulma-badge": "^1.0.1",
    "@creativebulma/bulma-tooltip": "^1.2.0",
    "@dfinity/agent": "0.15.3",
    "@dfinity/auth-client": "^0.15.3",
    "@dfinity/candid": "0.15.3",
    "@dfinity/identity": "^0.15.3",
    "@dfinity/principal": "0.15.3",
    "@formatjs/cli": "^5.1.3",
    "@formatjs/ts-transformer": "^3.11.1",
    "@types/file-saver": "^2.0.5",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.4",
    "@types/react-modal": "^3.13.1",
    "assert": "2.0.0",
    "buffer": "6.0.3",
    "bulma": "^0.9.3",
    "bulma-checkradio": "^2.1.3",
    "bulma-switch": "^2.0.4",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.26.0",
    "eslint-plugin-formatjs": "^4.3.4",
    "events": "3.3.0",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "8.0.0",
    "process": "0.11.10",
    "sass-loader": "13.2.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "stream-browserify": "3.0.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "5.2.5",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "util": "0.12.4",
    "webpack": "5.63.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "^4.4.0"
  },
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version",
    "last 2 safari version",
    "last 2 edge version"
  ],
