{
  "name": "dchanges_assets",
  "version": "0.1.0",
  "description": "Internet Computer starter application",
  "keywords": [
    "Internet Computer",
    "Motoko",
    "JavaScript",
    "Canister"
  ],
  "scripts": {
    "build": "NODE_ENV=production npm run css-build && webpack",
    "prebuild": "npm run copy:types",
    "css-build": "node-sass --omit-source-map-url src/dchanges_assets/sass/mystyles.scss src/dchanges_assets/assets/mystyles.css",
    "css-watch": "npm run css-build -- --watch",
    "dev": "npm run css-build && npm run serve",
    "dev:app": "dfx deploy dchanges && dfx deploy dchanges_assets",
    "dev:ii": "dfx deploy internet_identity --no-wallet --argument '(null)'",
    "serve": "webpack serve --mode development --env development",
    "prestart": "npm run copy:types",
    "copy:types": "rsync -avr .dfx/$(echo ${DFX_NETWORK:-'**'})/canisters/** --exclude='assets/' --exclude='idl/' --exclude='*.wasm' --exclude='*.most' --delete src/declarations"
  },
  "devDependencies": {
    "@creativebulma/bulma-badge": "^1.0.1",
    "@creativebulma/bulma-tooltip": "^1.2.0",
    "@dfinity/agent": "0.11.1",
    "@dfinity/auth-client": "^0.11.1",
    "@dfinity/authentication": "^0.11.1",
    "@dfinity/candid": "0.11.1",
    "@dfinity/identity": "^0.11.1",
    "@dfinity/principal": "0.11.1",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.4",
    "@types/react-modal": "^3.13.1",
    "assert": "2.0.0",
    "buffer": "6.0.3",
    "bulma": "^0.9.3",
    "bulma-checkradio": "^2.1.3",
    "bulma-switch": "^2.0.4",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^7.1.0",
    "events": "3.3.0",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "html-webpack-plugin": "5.5.0",
    "node-sass": "^7.0.1",
    "process": "0.11.10",
    "sass-loader": "^12.6.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "stream-browserify": "3.0.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "5.2.5",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "util": "0.12.4",
    "webpack": "5.63.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "^4.4.0"
  },
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version",
    "last 2 safari version",
    "last 2 edge version"
  ],
  "dependencies": {
    "bulma-o-steps": "^1.1.0",
    "bulma-toast": "^2.4.1",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.1",
    "easymde": "^2.16.1",
    "line-awesome": "^1.3.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-markdown": "^8.0.3",
    "react-query": "^3.38.1",
    "react-router-dom": "^6.3.0",
    "react-simplemde-editor": "^5.0.2",
    "yup": "^0.32.11"
  }
}
